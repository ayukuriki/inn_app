
<%=form_with url:login_path do |f| %>

 <div class= "form-container">
 <div class= "form-card">
    <table>
     <h3>ログイン</h3>
            <tr>
            <td ><%= f.text_field :name, placeholder: "ユーザー名",class: "searchbox" %></td>
            </tr>
            <tr>
            <td ><%= f.text_field :email, placeholder: "メールアドレス", class: "searchbox" %></zwtd>
            </tr>
     </table>
            <div>
                <ul>
                <li><%= f.submit "ログイン" ,class: "btn btn-primary" %></li>
                <li><%= link_to "新しいアカウントを作成", new_user_path, class:"userList"%></li>
                </ul>
            </div>
  </div>
</div>
<% end %>