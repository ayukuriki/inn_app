<% @rooms.each do |room| %>  
    <table>
     <h3>登録済みルーム一覧</h3>
        <thead>
        <tr>
            <th>ルーム画像</th>
            <th>ルーム名</th>
            <th>ルーム紹介</th>
            <th>料金</th>
            <th>作成日</th>
        </tr>
        </thead>

        <tbody>
        <% @rooms.each do |room| %>
            <tr>
            <td><%= link_to @room.image,room_path  %></td>
            <td><%= link_to @room.name,room_path  %></td>
            <td><%= @room.introduction %></td>
            <td><%= @room.price %></td>
            <td><%= @room.created_at %></td>
            </tr>
        <% end %>
        </tbody>
  </table>

<% end %>